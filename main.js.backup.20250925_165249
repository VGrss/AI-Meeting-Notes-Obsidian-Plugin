  // eslint-disable-next-line @typescript-eslint/explicit-member-accessibility, jsdoc/require-jsdoc
  _processEntry(entry) {
    var _a;
    (_a = this._onBeforeProcessingEntry) == null ? void 0 : _a.call(this, entry);
  }
